        "# 02\n",
        "```\n",
        "from collections import deque\n",
        "\n",
        "def check_w_w_reverse(string):\n",
        "    q = deque()\n",
        "    i = 0\n",
        "\n",
        "    while string[i] != '$':\n",
        "        q.append(string[i])\n",
        "        i += 1\n",
        "    i += 1,
        "\n",
        "    while i < len(string):\n",
        "        if q.pop() != string[i]:\n",
        "            return False\n",
        "        i += 1\n",
        "    return True\n",
        "\n",
        "```\n",
